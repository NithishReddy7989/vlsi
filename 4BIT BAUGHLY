and g5 (w3, a[2], b[1]); 
and g6 (w4, a[1], b[1]); 
and g7 (w5, a[0], b[1]); 
and g8 (w12, a[3], b[2]); 
and g9 (w13, a[2], b[2]); 
and g10(w14, a[1], b[2]); 
and g11(w15, a[O], b[2]); 
