and g13(w22, a[2], b[3]); 
and g14(w23, a[1], b[3]); 
and g15(w24, a[0], b[3]); 
fa_df f0 (w8, w7, w0, w3, 1'b0); 
fa_df f1 (w10, w9, w1 , w4, 1'b0); 
fa_df f2 (p[1], w11 , w2, w5, 1'b0); 
