module bm(p, a,b); 
output [7:0] p; 
input [3:0] a,b; 
wire w0,w1,w2,w3,w4,w5,w6,w7,w8,w9,w10,w11,w12,w13,w14,w15,w16,w17, 
w18, w19,w20,w21,w22,w23,w24,w25,w26,w27,w28,w29; 
and g0 (w0, a[3], b[0]); 
and g1 (w1, a[2], b[0]); 
and g2 (w2, a[1], b[0]); 
and g3 (p[0],a[0], b[0]); 
